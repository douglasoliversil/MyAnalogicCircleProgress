<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomCircularProgressBar">
        <attr name="strokeColor" format="color" />
        <attr name="strokeWidth" format="dimension" />
        <attr name="pad" format="float" />
        <attr name="mode" format="enum">
            <enum name="analogic" value="0" />
            <enum name="digital" value="1" />
        </attr>
    </declare-styleable>
</resources>